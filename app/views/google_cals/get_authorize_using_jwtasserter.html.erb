<input type="button" value="Delete Event" onclick="delete_event();" />
<table border="1" cellpadding="0" cellspacing="0">
	<tr>
		<th>ID</th>
		<th>Event</th>
		<th>Organizer</th>
		<th>Attendees</th>
		<th>Start DateTime</th>
		<th>End DateTime</th>
	</tr>
	<% @events_hash.each do |e| %>
		<tr>
			<td><%= e["id"] %></td>
			<td><%= e["summary"] %></td>
			<td><%= e["organizer"] %></td>
			<td><%= e["attendees"] %></td>
			<td><%= e["start_date_time"] %></td>
			<td><%= e["end_date_time"] %></td>
		</tr>
	<% end %>
</table>
